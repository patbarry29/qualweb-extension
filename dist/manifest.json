{
  "name": "qualweb-extension",
  "description": "Accessibility evaluation using QualWeb",
  "version": "1.0.03",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.ico"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js",
        "act.js",
        "wcag.js",
        "qwPage.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "icons/logoQW.png"
  ],
  "devtools_page": "devtools.html"
}